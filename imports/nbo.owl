    </owl:Class>
    


    <!-- http://www.ndesignpatterns.org/ontologies/nbo.owl#Permission -->

    <owl:Class rdf:about="http://www.ndesignpatterns.org/ontologies/nbo.owl#Permission">
        <rdfs:label xml:lang="en">Permission</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.ndesignpatterns.org/ontologies/nbo.owl#Normative_value"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ndesignpatterns.org/ontologies/nbo.owl#is_violated_by"/>
                <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">0</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">It can never be violated but can be satisfied or not.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ndesignpatterns.org/ontologies/nbo.owl#Person -->

    <owl:Class rdf:about="http://www.ndesignpatterns.org/ontologies/nbo.owl#Person">
