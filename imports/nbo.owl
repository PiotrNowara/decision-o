        <rdfs:subClassOf rdf:resource="http://www.ndesignpatterns.org/ontologies/nbo.owl#Realizable"/>
    </owl:Class>
    


    <!-- http://www.ndesignpatterns.org/ontologies/nbo.owl#Policy -->

    <owl:Class rdf:about="http://www.ndesignpatterns.org/ontologies/nbo.owl#Policy">
        <rdfs:label xml:lang="en">Policy</rdfs:label>
        <rdfs:label xml:lang="pl">Polityka</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.ndesignpatterns.org/ontologies/nbo.owl#Plan"/>
        <rdfs:comment xml:lang="en">A plan of action adopted by an individual or social group.</rdfs:comment>
        <rdfs:comment xml:lang="pl">Np. polityka bezpieczeństwa.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ndesignpatterns.org/ontologies/nbo.owl#Privilege -->

    <owl:Class rdf:about="http://www.ndesignpatterns.org/ontologies/nbo.owl#Privilege">
        <rdfs:label xml:lang="en">Privilege</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.ndesignpatterns.org/ontologies/nbo.owl#Permission"/>
    </owl:Class>
    


