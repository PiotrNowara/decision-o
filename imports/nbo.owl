        <rdfs:subClassOf rdf:resource="http://www.ndesignpatterns.org/ontologies/nbo.owl#Permission"/>
        <rdfs:comment xml:lang="en">It can never be violated but can be satisfied or not</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ndesignpatterns.org/ontologies/nbo.owl#Recommendation_specification -->

    <owl:Class rdf:about="http://www.ndesignpatterns.org/ontologies/nbo.owl#Recommendation_specification">
        <rdfs:label xml:lang="en">Recommendation specification</rdfs:label>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ndesignpatterns.org/ontologies/nbo.owl#describes"/>
                <owl:someValuesFrom rdf:resource="http://www.ndesignpatterns.org/ontologies/nbo.owl#Recommendation"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.ndesignpatterns.org/ontologies/nbo.owl#Normative_specification"/>
    </owl:Class>
    


    <!-- http://www.ndesignpatterns.org/ontologies/nbo.owl#Requirement -->

    <owl:Class rdf:about="http://www.ndesignpatterns.org/ontologies/nbo.owl#Requirement">
        <rdfs:label xml:lang="en">Requirement</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.ndesignpatterns.org/ontologies/nbo.owl#Normative_value"/>
        <rdfs:comment xml:lang="en">A norm implying an obligation that is violated when not satisfied.</rdfs:comment>
        <toDo xml:lang="en">How obligation is different from a requirement?</toDo>
        <rdfs:comment xml:lang="en">Obligation setter</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ndesignpatterns.org/ontologies/nbo.owl#Requirement_specification -->

    <owl:Class rdf:about="http://www.ndesignpatterns.org/ontologies/nbo.owl#Requirement_specification">
        <rdfs:label xml:lang="en">Requirement specification</rdfs:label>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ndesignpatterns.org/ontologies/nbo.owl#describes"/>
                <owl:someValuesFrom rdf:resource="http://www.ndesignpatterns.org/ontologies/nbo.owl#Requirement"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.ndesignpatterns.org/ontologies/nbo.owl#Normative_specification"/>
    </owl:Class>
    


    <!-- http://www.ndesignpatterns.org/ontologies/nbo.owl#Resuming -->

    <owl:Class rdf:about="http://www.ndesignpatterns.org/ontologies/nbo.owl#Resuming">
        <rdfs:label xml:lang="en">Resuming</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.ndesignpatterns.org/ontologies/nbo.owl#Control_process"/>
    </owl:Class>
    


    <!-- http://www.ndesignpatterns.org/ontologies/nbo.owl#Risk -->

    <owl:Class rdf:about="http://www.ndesignpatterns.org/ontologies/nbo.owl#Risk">
        <rdfs:label xml:lang="en">Risk</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.ndesignpatterns.org/ontologies/nbo.owl#Situation"/>
        <rdfs:comment xml:lang="en">Combination of the frequency of occurrence of harm and the severity of that harm.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ndesignpatterns.org/ontologies/nbo.owl#Role -->

    <owl:Class rdf:about="http://www.ndesignpatterns.org/ontologies/nbo.owl#Role">
        <rdfs:label xml:lang="en">Role</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.ndesignpatterns.org/ontologies/nbo.owl#Realizable"/>
        <rdfs:comment xml:lang="pl">Rola</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ndesignpatterns.org/ontologies/nbo.owl#Satisfying_norm -->

    <owl:Class rdf:about="http://www.ndesignpatterns.org/ontologies/nbo.owl#Satisfying_norm">
        <rdfs:label xml:lang="en">Satisfying norm</rdfs:label>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ndesignpatterns.org/ontologies/nbo.owl#is_occurence_of"/>
                <owl:someValuesFrom>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.ndesignpatterns.org/ontologies/nbo.owl#satisfies"/>
                        <owl:someValuesFrom rdf:resource="http://www.ndesignpatterns.org/ontologies/nbo.owl#Normative_value"/>
                    </owl:Restriction>
                </owl:someValuesFrom>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.ndesignpatterns.org/ontologies/nbo.owl#Situation"/>
    </owl:Class>
    


    <!-- http://www.ndesignpatterns.org/ontologies/nbo.owl#Sign -->

    <owl:Class rdf:about="http://www.ndesignpatterns.org/ontologies/nbo.owl#Sign">
        <rdfs:label xml:lang="en">Sign</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.ndesignpatterns.org/ontologies/nbo.owl#Physical_object"/>
    </owl:Class>
    


    <!-- http://www.ndesignpatterns.org/ontologies/nbo.owl#Situation -->

    <owl:Class rdf:about="http://www.ndesignpatterns.org/ontologies/nbo.owl#Situation">
        <rdfs:label xml:lang="en">Situation</rdfs:label>
        <rdfs:label xml:lang="pl">Sytuacja</rdfs:label>
        <rdfs:subClassOf rdf:resource="&owl;Thing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ndesignpatterns.org/ontologies/nbo.owl#is_occurence_of"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.ndesignpatterns.org/ontologies/nbo.owl#Person"/>
                            <rdf:Description rdf:about="http://www.ndesignpatterns.org/ontologies/nbo.owl#Physical_object"/>
                            <rdf:Description rdf:about="http://www.ndesignpatterns.org/ontologies/nbo.owl#Process"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.ndesignpatterns.org/ontologies/nbo.owl#Standard -->

    <owl:Class rdf:about="http://www.ndesignpatterns.org/ontologies/nbo.owl#Standard">
        <rdfs:label xml:lang="en">Standard</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.ndesignpatterns.org/ontologies/nbo.owl#Normative_value"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ndesignpatterns.org/ontologies/nbo.owl#has_constituent"/>
                <owl:someValuesFrom rdf:resource="http://www.ndesignpatterns.org/ontologies/nbo.owl#Normative_value"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Something that consists of normative values (for example requirements and recommendations) and can be viewed itself as a normative value.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ndesignpatterns.org/ontologies/nbo.owl#State_of_a_process -->

    <owl:Class rdf:about="http://www.ndesignpatterns.org/ontologies/nbo.owl#State_of_a_process">
        <rdfs:label xml:lang="en">State of a process</rdfs:label>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ndesignpatterns.org/ontologies/nbo.owl#is_state_of"/>
                <owl:someValuesFrom rdf:resource="http://www.ndesignpatterns.org/ontologies/nbo.owl#Process"/>
