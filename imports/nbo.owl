        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ndesignpatterns.org/ontologies/nbo.owl#is_opportunity_of"/>
                <owl:someValuesFrom rdf:resource="http://www.ndesignpatterns.org/ontologies/nbo.owl#Person"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


