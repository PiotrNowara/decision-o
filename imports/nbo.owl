        <rdfs:seeAlso rdf:resource="http://www.ndesignpatterns.org/ontologies/nbo.owl#Decision_making"/>
        <rdfs:seeAlso rdf:resource="http://www.ndesignpatterns.org/ontologies/nbo.owl#has_criterion"/>
    </owl:Class>
    


