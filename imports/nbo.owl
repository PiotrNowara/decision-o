    </owl:Class>
    


    <!-- http://www.ndesignpatterns.org/ontologies/nbo.owl#Pursue -->

    <owl:Class rdf:about="http://www.ndesignpatterns.org/ontologies/nbo.owl#Pursue">
        <rdfs:label xml:lang="en">Pursue</rdfs:label>
        <rdfs:label xml:lang="pl">Dążenie</rdfs:label>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ndesignpatterns.org/ontologies/nbo.owl#is_pursue_for"/>
                <owl:someValuesFrom rdf:resource="http://www.ndesignpatterns.org/ontologies/nbo.owl#Purpose"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.ndesignpatterns.org/ontologies/nbo.owl#Attitude"/>
    </owl:Class>
    


    <!-- http://www.ndesignpatterns.org/ontologies/nbo.owl#Question -->

    <owl:Class rdf:about="http://www.ndesignpatterns.org/ontologies/nbo.owl#Question">
        <rdfs:label xml:lang="en">Question</rdfs:label>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ndesignpatterns.org/ontologies/nbo.owl#is_question_for"/>
                <owl:someValuesFrom rdf:resource="&owl;Thing"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.ndesignpatterns.org/ontologies/nbo.owl#Situation"/>
        <rdfs:comment xml:lang="en">A situation in which a person indicates a will for acquiring knowledge about something.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ndesignpatterns.org/ontologies/nbo.owl#Question_for_confirming -->

    <owl:Class rdf:about="http://www.ndesignpatterns.org/ontologies/nbo.owl#Question_for_confirming">
        <rdfs:label xml:lang="en">Question for confirming</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.ndesignpatterns.org/ontologies/nbo.owl#is_question_for_confirming"/>
                        <owl:someValuesFrom rdf:resource="&owl;Thing"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.ndesignpatterns.org/ontologies/nbo.owl#has_answer"/>
                        <owl:allValuesFrom>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.ndesignpatterns.org/ontologies/nbo.owl#indicates_boolean_value"/>
                                <owl:someValuesFrom rdf:resource="&xsd;boolean"/>
                            </owl:Restriction>
                        </owl:allValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.ndesignpatterns.org/ontologies/nbo.owl#Question"/>
        <owl:disjointWith rdf:resource="http://www.ndesignpatterns.org/ontologies/nbo.owl#Question_for_indicating"/>
        <rdfs:comment xml:lang="en">A question containing a whole proposition, that is an utterance that could be true or false. It is answerd by confirming or denying that proposition (&#39;yes/no question&#39;)

Examples: Are you a vegetarian? Is Norway a member of UE?</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ndesignpatterns.org/ontologies/nbo.owl#Question_for_indicating -->

    <owl:Class rdf:about="http://www.ndesignpatterns.org/ontologies/nbo.owl#Question_for_indicating">
        <rdfs:label xml:lang="en">Question for indicating</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.ndesignpatterns.org/ontologies/nbo.owl#is_question_for_indicating"/>
                        <owl:someValuesFrom rdf:resource="&owl;Thing"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.ndesignpatterns.org/ontologies/nbo.owl#has_answer"/>
                        <owl:allValuesFrom>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.ndesignpatterns.org/ontologies/nbo.owl#indicates_chosen_option"/>
                                <owl:someValuesFrom rdf:resource="&owl;Thing"/>
                            </owl:Restriction>
                        </owl:allValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.ndesignpatterns.org/ontologies/nbo.owl#Question"/>
        <rdfs:comment xml:lang="en">A question demanding indication of some concept, thing or its property in order to be anwsered.

Examples: What&#39;s your date of birth? What is the name of the Director of the W3C?</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ndesignpatterns.org/ontologies/nbo.owl#Question_for_indicating_solution -->

    <owl:Class rdf:about="http://www.ndesignpatterns.org/ontologies/nbo.owl#Question_for_indicating_solution">
        <rdfs:label xml:lang="en">Question_for_indicating solution</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.ndesignpatterns.org/ontologies/nbo.owl#Question_for_indicating"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ndesignpatterns.org/ontologies/nbo.owl#is_consequence_of"/>
                <owl:someValuesFrom rdf:resource="http://www.ndesignpatterns.org/ontologies/nbo.owl#Problem"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.ndesignpatterns.org/ontologies/nbo.owl#Rate -->

    <owl:Class rdf:about="http://www.ndesignpatterns.org/ontologies/nbo.owl#Rate">
        <rdfs:label xml:lang="en">Rate</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.ndesignpatterns.org/ontologies/nbo.owl#Feature"/>
    </owl:Class>
    


    <!-- http://www.ndesignpatterns.org/ontologies/nbo.owl#Readiness -->

    <owl:Class rdf:about="http://www.ndesignpatterns.org/ontologies/nbo.owl#Readiness">
        <rdfs:label xml:lang="en">Readiness</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.ndesignpatterns.org/ontologies/nbo.owl#Attitude"/>
    </owl:Class>
    


    <!-- http://www.ndesignpatterns.org/ontologies/nbo.owl#Realizable -->

    <owl:Class rdf:about="http://www.ndesignpatterns.org/ontologies/nbo.owl#Realizable">
        <rdfs:label xml:lang="en">Realizable</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.ndesignpatterns.org/ontologies/nbo.owl#has_realization"/>
                        <owl:someValuesFrom rdf:resource="http://www.ndesignpatterns.org/ontologies/nbo.owl#Process"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.ndesignpatterns.org/ontologies/nbo.owl#has_realization"/>
                        <owl:someValuesFrom rdf:resource="http://www.ndesignpatterns.org/ontologies/nbo.owl#Situation"/>
                    </owl:Restriction>
                </owl:unionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="&owl;Thing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ndesignpatterns.org/ontologies/nbo.owl#has_bearer"/>
                <owl:someValuesFrom rdf:resource="&owl;Thing"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">[description] Something that can be actualized.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ndesignpatterns.org/ontologies/nbo.owl#Recommendation -->

    <owl:Class rdf:about="http://www.ndesignpatterns.org/ontologies/nbo.owl#Recommendation">
